{
  "name": "MyBookMarkSearch",
  "description": "BookMark のSearch をいい感じにするやーつ",
  "version": "0.0.1",
  "manifest_version": 3,
  "permissions": [
    "bookmarks"
  ],
  "action": {
    "default_title": "⌨➡ [Ctrl] + [Shift] + [Space]",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": "Ctrl+Shift+Space",
      "description": "Start up"
    }
  },
  "icons": {
    "16": "icon.png"
  }
}